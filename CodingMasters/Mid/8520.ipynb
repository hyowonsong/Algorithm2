   "source": [
    "# 8520 컴퓨터 학원\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "def solution(N):\n",
    "    if N == 1:\n",
    "        return 3\n",
    "    dp = [0] * (N + 1)\n",
    "    dp[1] = 3\n",
    "    dp[2] = 7\n",
    "    \n",
    "    for i in range(3, N + 1):\n",
    "        dp[i] = (dp[i-1] * 2 + dp[i-2]) % 796796\n",
    "    \n",
    "    return dp[N]\n",
    "\n",
    "print(solution(N))"
   ]
  }
