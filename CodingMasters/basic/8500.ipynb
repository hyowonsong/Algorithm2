    "# 8500 조합\n",
    "\n",
    "import math\n",
    "\n",
    "# 입력값을 받습니다.\n",
    "A, B = map(int, input().split())\n",
    "\n",
    "def combinations(A, B):\n",
    "    return math.factorial(A)// (math.factorial(B) * math.factorial(A - B))\n",
    "print(combinations(A, B))"
   ]
